  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@types/ramda": "^0.27.44",
    "jest": "^27.0.6",
    "ts-jest": "^27.0.4",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  },
